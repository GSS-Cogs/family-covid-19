@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix cogsgit: <https://github.com/GSS-Cogs/> .
@prefix cogs: <http://gss-cogs.uk/def/arch/> .

<http://gss-cogs.uk/def/arch> a owl:Ontology;
  rdfs:label "COGS COVID-19 Transform Mapping" ;
.

cogs:SoftwarePackage a owl:Class ;
  rdfs:label "Software package" ;
.
cogs:LandingPage a owl:Class ;
  rdfs:subClassOf cogs:SoftwarePackage ;
  rdfs:label "Landing Page" ;
.
cogs:Datasets a owl:Class ;
  rdfs:subClassOf cogs:SoftwarePackage ;
  rdfs:label "Output Dataset" ;
.
cogs:Sheets a owl:Class ;
  rdfs:subClassOf cogs:SoftwarePackage ;
  rdfs:label "Sheet names to join";
.
cogs:Columns a owl:Class ;
  rdfs:subClassOf cogs:SoftwarePackage ;
  rdfs:label "Table" ;
.
cogs:PMD a owl:Class ;
  rdfs:subClassOf cogs:SoftwarePackage ;
  rdfs:label "PMD" ;
.

###########################################################################################################

cogsgit:top-level a cogs:LandingPage ;
  rdfs:label "Notifications of deaths of residents related to COVID-19 in adult care homes" ;
  rdfs:comment "https://gov.wales/notifications-deaths-residents-related-covid-19-adult-care-homes";
  cogs:splits cogsgit:sheet1, cogsgit:sheet2 ;
.

cogsgit:sheet1 a cogs:Sheets ;
  rdfs:label "Table_1" ;
  rdfs:comment "Notifications of Service User Deaths received from Adult Care Homes" ;
  cogs:transforms cogsgit:dataset1 ;
.
cogsgit:sheet2 a cogs:Sheets ;
  rdfs:label "Table_2" ;
  rdfs:comment "Notifications of deaths of adult care home residents with confirmed or suspected COVID-19 by location of death" ;
  cogs:transforms cogsgit:dataset1 ;
.
cogsgit:sheet2 a cogs:Sheets ;
  rdfs:label "Table_3" ;
  rdfs:comment "Notifications of deaths of residents from adult care homes by date of notification and cause" ;
  cogs:transforms cogsgit:dataset1 ;
.
cogsgit:sheet2 a cogs:Sheets ;
  rdfs:label "Table_4" ;
  rdfs:comment "Notifications of deaths of adult care home residents with confirmed or suspected covid-19 by location of death and date of notification" ;
  cogs:transforms cogsgit:dataset1 ;
.
cogsgit:sheet2 a cogs:Sheets ;
  rdfs:label "Table_5" ;
  rdfs:comment "Notifications of deaths of adult care home residents by location of death and date of notification" ;
  cogs:transforms cogsgit:dataset1 ;
.
cogsgit:sheet2 a cogs:Sheets ;
  rdfs:label "Table_6" ;
  rdfs:comment "Number of notifications of deaths of adult care home residents involving COVID-19 (both confirmed and suspected) occurring in care homes, by Local Authority and day of notification" ;
  cogs:transforms cogsgit:dataset1 ;
.
cogsgit:sheet2 a cogs:Sheets ;
  rdfs:label "Table_7" ;
  rdfs:comment "Number of notifications of deaths of adult care home residents involving COVID-19 (both confirmed and suspected) occurring in any location, by Local Authority and day of notification" ;
  cogs:transforms cogsgit:dataset1 ;
.
cogsgit:sheet2 a cogs:Sheets ;
  rdfs:label "Table_8" ;
  rdfs:comment "Number of notifications of deaths of adult care home residents by Local Authority and day of notification" ;
  cogs:transforms cogsgit:dataset1 ;
.
cogsgit:sheet2 a cogs:Sheets ;
  rdfs:label "Table_9" ;
  rdfs:comment "Number of notifications of deaths of adult care home residents occurring in care homes by Local Authority and day of notification" ;
  cogs:transforms cogsgit:dataset1 ;
.


cogsgit:dataset1 a cogs:Datasets ;
  rdfs:label "" ;
  rdfs:comment "WG NHS activity and capacity during the coronavirus COVID-19 pandemic" ;
  cogs:outputs cogsgit:columns1 ;
.


cogsgit:columns1 a cogs:Columns ;
  rdfs:label "" ;
  rdfs:comment "Period, Local Health Board, Case Count Type, Measure Type, Unit, Marker, Value" ;
  cogs:uploads cogsgit:pmd1 ;
.


cogsgit:pmd1 a cogs:PMD ;
  rdfs:label "" ;
  rdfs:comment "Publish my Data: http://gss-data.org.uk/data/search" ;
.
###################################################################################################
